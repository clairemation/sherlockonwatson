<div id="photos-box">
  <% @people.each do |person| %>
    <div class="each_person">
    <%= image_tag person.image_url, alt: person.name, class: "person_photo" %>
    <p><%= person.name %></p>
    </div>
  <% end %>
</div>
<button id="report-button" type="button">REPORT</button>
<form id="image" method="post" enctype="multipart/form-data">
<!--   <input type="hidden" name="parameters" value = ""> -->
  <input type="file" id="imageFile" name="images_file" accept="image/*" capture = "camera">
  <input type="submit">
</form>

<div id="results">
</div>

